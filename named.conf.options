options {
    directory "/var/cache/bind";

    recursion yes;
    listen-on { 127.0.0.1; };
    dnssec-validation auto;

    // get our requests off of the UW campus network.
    // use google, then cloudflare, then google.
    forwarders {
        8.8.8.8;
        1.1.1.1;
        8.8.4.4;
    };
};
